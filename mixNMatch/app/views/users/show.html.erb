<p id="notice"><%= notice %></p>

<div>
    <% if current_user %>
        <h2> Your Account </h2>
        <p>
            <strong>Email:</strong>
            <%= current_user[:email] %>
        </p>
        <h3> Your Profile </h4>
        <p>
            <strong>Description: </strong>
            <%= @profile[:description] %>
        <p>
        <p>
            <strong>Song: </strong>
            <%= @profile[:song] %>
